for (int row = 0; row <= foodArr.size(); row++) {
    for (int col = 0; col <= columnNames.length; col++) {
        Label label;

        if (row == 0) {
            // Header row
            if (col == columnNames.length) {
                continue; // Skip the last column for the delete button
            }
            label = new Label(columnNames[col]);
            grid.add(label, col, row);
        } else if (col == columnNames.length) {
            // Delete button column
            delete = new Button("Delete");
            grid.add(delete, col, row);
        } else {
            // Data row
            Food currentFood = foodArr.get(row - 1); // row - 1 because row 0 is the header

            if (columnNames[col].equals("Name")) {
                // Get the name of the food
                label = new Label(currentFood.getName());
            } else {
                // Get the macro value
                SimpleIntegerProperty word = currentFood.getMacro(columnNames[col]);
                label = new Label(word.getValue().toString());
            }
            grid.add(label, col, row);
        }
for (int row = 0; row <= foodArr.size(); row++) {
    for (int col = 0; col <= columnNames.length; col++) {
        Label label;
        if (row == 0) {
            // Header row
            if (col == columnNames.length) {
                continue; // Skip the last column for the delete button
            }
            label = new Label(columnNames[col]);
            grid.add(label, col, row);
        } else if (col == columnNames.length) {
            // Delete button column
            delete = new Button("Delete");
            grid.add(delete, col, row);
        } else {
            // Data rows
            Food currentFood = foodArr.get(row - 1); // Fetch correct Food instance
            if (columnNames[col].equals("Name")) {
                label = new Label(currentFood.NameProperty().getValue()); // Use instance method
            } else {
                SimpleIntegerProperty word = currentFood.getMacro(columnNames[col]); // Use instance method
                label = new Label(word.getValue().toString());
            }
            grid.add(label, col, row);
        }
    }
}
